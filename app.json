{
  "expo": {
    "name": "Lakad",
    "slug": "thesis-lakad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lakad-icon.png",
    "scheme": "thesislakadrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bulakero.lakad"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/lakad-icon-foreground.png",
        "backgroundImage": "./assets/images/lakad-icon-background.png",
        "monochromeImage": "./assets/images/lakad-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bulakero.lakad",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "thesislakadrn",
              "host": "auth"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/lakad-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lakad-icon-foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      "@rnmapbox/maps"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}